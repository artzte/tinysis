%tr
  %td.name= link_to(student.last_name_first, student_status_path(student))
  %td.coordinator= student.coordinator ? student.coordinator.last_name_f : 'Unassigned'
  %td.grade= student.district_grade
  %td.links
    = link_to "ALE Report", "#"
    = link_to "Learning Plan", "#"
    = link_to "Credits", "#"
    = link_to "Graduation Plan", "$"
